{
  "name": "bamboost",
  "version": "0.3.0",
  "private": true,
  "type": "module",
  "scripts": {
    "quarto-build": "mkdir -p .docs/ && rm -r .docs/** && bash content/_scripts/copyMdx.sh content/docs .docs/docs && quarto render content --execute",
    "build": "next build",
    "dev": "concurrently \"node content/_scripts/watchMdx.js\" \"quarto preview content\"",
    "start": "next start",
    "postinstall": "fumadocs-mdx",
    "sync-oramacloud": "node --env-file=.env scripts/update-orama.ts",
    "create-source": "tsx fumapy/create-source.ts"
  },
  "dependencies": {
    "@orama/core": "^1.2.13",
    "ansi-to-html": "^0.7.2",
    "chokidar": "^5.0.0",
    "concurrently": "^9.2.1",
    "fumadocs-core": "16.2.3",
    "fumadocs-mdx": "14.1.0",
    "fumadocs-python-autodoc": "^0.0.3",
    "fumadocs-ui": "16.2.3",
    "geist": "^1.5.1",
    "next": "^16.0.7",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "rehype-raw": "^7.0.0"
  },
  "devDependencies": {
    "@oramacloud/client": "^2.1.4",
    "@tailwindcss/postcss": "^4.1.17",
    "@types/mdx": "^2.0.13",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/rehype-react": "^6.0.0",
    "clsx": "^2.1.1",
    "fs-extra": "^11.3.2",
    "hast-util-to-jsx-runtime": "^2.3.6",
    "katex": "^0.16.27",
    "lucide-react": "^0.556.0",
    "mdast-util-to-string": "^4.0.0",
    "postcss": "^8.5.6",
    "rehype-katex": "^7.0.1",
    "rehype-parse": "^9.0.1",
    "rehype-react": "^8.0.0",
    "remark-gfm": "^4.0.1",
    "remark-math": "^6.0.0",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.2",
    "shiki": "^3.19.0",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.17",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
