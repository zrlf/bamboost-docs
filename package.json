{
  "name": "bamboost",
  "version": "0.3.0",
  "private": true,
  "type": "module",
  "scripts": {
    "quarto-build": "mkdir -p .docs/ && rm -r .docs/** && bash content/_scripts/copyMdx.sh content/docs .docs/docs && quarto render content --execute",
    "build": "tsx fumapy/create-source.ts && next build",
    "dev": "concurrently \"node content/_scripts/watchMdx.js\" \"quarto preview content\"",
    "start": "next start",
    "postinstall": "fumadocs-mdx",
    "create-source": "tsx fumapy/create-source.ts"
  },
  "dependencies": {
    "ansi-to-html": "^0.7.2",
    "chokidar": "^4.0.3",
    "concurrently": "^9.1.2",
    "fumadocs-core": "15.3.0",
    "fumadocs-mdx": "11.6.3",
    "fumadocs-ui": "15.3.0",
    "geist": "^1.4.2",
    "next": "^15.3.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "rehype-raw": "^7.0.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.6",
    "@types/mdx": "^2.0.13",
    "@types/node": "^22.15.17",
    "@types/react": "^19.1.3",
    "@types/react-dom": "^19.1.3",
    "@types/rehype-react": "^6.0.0",
    "clsx": "^2.1.1",
    "fs-extra": "^11.3.0",
    "hast-util-to-jsx-runtime": "^2.3.6",
    "katex": "^0.16.22",
    "lucide-react": "^0.509.0",
    "mdast-util-to-string": "^4.0.0",
    "postcss": "^8.5.3",
    "rehype-katex": "^7.0.1",
    "rehype-parse": "^9.0.1",
    "rehype-react": "^8.0.0",
    "remark-gfm": "^4.0.1",
    "remark-math": "^6.0.0",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.2",
    "shiki": "^3.4.0",
    "tailwind-merge": "^3.2.0",
    "tailwindcss": "^4.1.6",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "unified": "^11.0.5",
    "unist-util-visit": "^5.0.0"
  }
}
